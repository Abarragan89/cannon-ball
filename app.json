{
  "expo": {
    "scheme": "cannonballsea",
    "name": "Cannon Ball Sea",
    "slug": "cannon-ball-sea",
    "version": "1.0.0",
    "orientation": "landscape",
    "backgroundColor": "#b59a57",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mathfactmissions.cannonBallSea"
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mathfactmissions.cannonBallSea",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/titleFont.ttf",
            "./assets/fonts/textFont.ttf"
          ]
        }
      ],
      [
        "expo-av"
      ],
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "0d70880e-9d83-40dd-ba0f-885797423b1d"
      }
    },
    "owner": "ant_dev"
  }
}
